{
    "name": "serviette",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "gulp build --gulpfile .build/Gulpfile.js",
        "watch": "gulp watch --gulpfile .build/Gulpfile.js",
        "dev": "concurrently \"npm:watch\" \"ts-node-dev --respawn --transpile-only .build/server.ts\"",
        "start": "ts-node .build/server.ts"
    },
    "dependencies": {
        "@types/cors": "^2.8.10",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "ts-node": "^9.1.1",
        "tslib": "^2.1.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "concurrently": "^6.0.0",
        "del": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "ts-node-dev": "^1.1.1",
        "typescript": "~3.9.3"
    }
}
